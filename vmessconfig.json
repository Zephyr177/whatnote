{
  "log": {
    "loglevel": "debug",
    "access": "/var/log/v2ray/access.log", // 这是 Linux 的路径
    "error": "/var/log/v2ray/error.log"
  },
  "inbounds": [
    {
      "sniffing": {
        "enabled": true,
        "destOverride": [
          "http",
          "tls"
        ]
      },
      "port": 25565, // 服务器监听端口
      "protocol": "vmess",    // 主传入协议
      "settings": {
        "clients": [
          {
            "id": PassWordForChange,  
            "alterId": 0
          }
        ]
      }
    }
  ],
  "outbounds": [
    {
      "protocol": "freedom",  // 主传出协议
      "settings": {},
      "tag": "freedom"
    },
    {
      "protocol": "blackhole",
      "settings": {},
      "tag": "blackhole"
    }
  ],
  "routing": {
    "rules": [
      {
         "type": "field",
          "ip": [
            "209.222.114.0/24"
            ]
         "outboundTag": "freedom"
      },
      {
         "type": "field",
         "protocol": ["http", "tls", "bittorrent"],
         "outboundTag": "blackhole"
      },
    ]
  }
}
